#main
  .notes_board.grid_11.alpha
    #main.grid_6.alpha
      %h3 Check It Out
      - @how_tos.each do |how_to|
        .home_note
          = render :partial => "how_tos/how_to_teaser", :locals => { :how_to => how_to }
    #main.grid_5.omega
      %h3 U Can Help
      %ul.question_list
        - @questions.each do |question|
          %li
            = link_to question.title, question_path(question)